server {
	server_name againfm.dev .localtunnel.com;

	location / {
		proxy_pass http://127.0.0.1:5000;
		proxy_set_header Host $host;
	}

	location /static {
		alias /var/www/againfm/afm/static;
	}

	location /admin/static {
		expires 7d;
		add_header Cache-Control public;
		alias /var/www/againfm/afm/admin/static;
	}
}
